<!DOCTYPE html>
<html>
    <% include ../partials/head %>
  <body>
    
    <% include ../partials/nav %>

    <div class="row">

        <% for(let i = 0; i < members.length; i++) { %>
            <% console.log(members[i]) %>
          <div class="col">      
              <div class="card" style="width: 18rem;">
                  <a href="/api/member/<%= members[i]._id %>"> 
                    <img class="card-img-top" src="<%= members[i].image %>" alt="Card image cap" />
                  </a> 
                  <div class="card-body">
                    <h5 class="card-title">Name:  <%= members[i].name %></h5>
                    <p class="card-text">Category: <%= members[i].category.name %></p>
                    <a href="/api/member/<%= members[i]._id %>" class="btn btn-primary">Shop</a>
                  </div>
              </div>
          </div>

        <% } %>
    </div>
  </body>
</html>